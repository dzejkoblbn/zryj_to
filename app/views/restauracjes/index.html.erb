<p id="notice"><%= notice %></p>

<h1>Restauracjes</h1>

<table>
  <thead>
    <tr>
      <th>Id restauracji</th>
      <th>Nazwa</th>
      <th>Ulica</th>
      <th>Numer lokalu</th>
      <th>Kod pocztowy</th>
      <th>Pracownicy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restauracjes.each do |restauracje| %>
      <tr>
        <td><%= restauracje.id_restauracji %></td>
        <td><%= restauracje.nazwa %></td>
        <td><%= restauracje.ulica %></td>
        <td><%= restauracje.numer_lokalu %></td>
        <td><%= restauracje.kod_pocztowy %></td>
        <td><%= restauracje.pracownicy %></td>
        <td><%= link_to 'Show', restauracje %></td>
        <td><%= link_to 'Edit', edit_restauracje_path(restauracje) %></td>
        <td><%= link_to 'Destroy', restauracje, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Restauracje', new_restauracje_path %>
