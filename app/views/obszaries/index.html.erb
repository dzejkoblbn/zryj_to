<p id="notice"><%= notice %></p>

<h1>Obszaries</h1>

<table>
  <thead>
    <tr>
      <th>Id obszaru</th>
      <th>Nazwa obszaru</th>
      <th>Restauracje</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @obszaries.each do |obszary| %>
      <tr>
        <td><%= obszary.id_obszaru %></td>
        <td><%= obszary.nazwa_obszaru %></td>
        <td><%= obszary.restauracje %></td>
        <td><%= link_to 'Show', obszary %></td>
        <td><%= link_to 'Edit', edit_obszary_path(obszary) %></td>
        <td><%= link_to 'Destroy', obszary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Obszary', new_obszary_path %>
